{
  "name": "believablecreations",
  "version": "15.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ngx-focus-point:library": "ng build @believablecreations/ngx-focus-point",
    "test:ngx-focus-point:library": "ng test @believablecreations/ngx-focus-point",
    "lint:ngx-focus-point:library": "ng lint @believablecreations/ngx-focus-point",
    "publish:ngx-focus-point:library": "cd dist/ngx-focus-point && npm publish --access public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run believablecreations:serve-ssr",
    "serve:ssr": "node dist/believablecreations/server/main.js",
    "build:ssr": "ng build && ng run believablecreations:server",
    "prerender": "ng run believablecreations:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/platform-server": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@nguniversal/express-engine": "^15.1.0",
    "express": "^4.15.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.1",
    "@angular/cli": "~15.1.1",
    "@angular/compiler-cli": "^15.1.0",
    "@nguniversal/builders": "^15.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.1",
    "typescript": "~4.8.2"
  }
